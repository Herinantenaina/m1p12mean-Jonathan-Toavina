{
  "version": 2,
  "builds": [
    {
      "src": "dist/frontend/server/main.server.mjs", 
      "use": "@vercel/node",
      "config": { 
        "includeFiles": [
          "dist/browser/**"   
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "dist/frontend/server/main.server.mjs"  
    }
  ]
}
